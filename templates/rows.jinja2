{%if fixed_navbar%}
    {% extends "./templates/base-fixed-navbar.jinja2" %}
{%elif (site_branch == "team")%}
    {% extends "./templates/team.jinja2" %}
{%else%}
    {% extends "./templates/base.jinja2" %}
{%endif%}
{% block content %}
    <div class="container">
        {%- for row in body -%}
            <div class="row">
                <div class="content" style="display:flex">{{row}}</div>
            </div>
        {%- endfor -%}
    </div>
{% endblock %}